{
  "name": "chav",
  "version": "0.0.1",
  "description": "chav is a hapi plugin to create static routes from json files",
  "main": "lib/index.js",
  "scripts": {
    "test": "./node_modules/.bin/lab -a code -t 100 -L -r console -o stdout -r lcov -o coverage/lcov.info",
    "test-html": "mkdir -p coverage && ./node_modules/.bin/lab -a code -t 100 -L -r html > coverage/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:igorkosta/chav.git"
  },
  "keywords": [
    "hapi",
    "static",
    "json",
    "plugin"
  ],
  "author": "igorkosta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igorkosta/chav/issues"
  },
  "homepage": "https://github.com/igorkosta/chav",
  "dependencies": {
    "boom": "^4.0.0",
    "hoek": "^4.0.2",
    "joi": "^9.0.4",
    "jsonfile": "^2.3.1"
  },
  "devDependencies": {
    "code": "^3.0.2",
    "hapi": "^15.0.3",
    "lab": "^11.0.1"
  }
}
